pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Coding') {
            steps {
                echo 'Coding'
            }
        }
         stage('Develop') {
            steps {
                echo 'Developing'
            }
        }
         stage('test') {
            steps {
                echo 'Testing'
            }
        }
    }
}
